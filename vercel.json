{
    "version": 2,
    "builds": [
      {
        "src": "build/*",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      }
    ],
    "routes": [
      { "src": "/(.*).(js|css|jpg|png|ico)", "headers": { "cache-control": "s-maxage=31536000" } },
      { "src": "/api/(.*)", "dest": "/api/index.js" },
      { "src": "/(.*)", "dest": "/build/$1" }
    ]
  }
  